{
  "name": "shades",
  "version": "1.13.0",
  "description": "Lens-like functionality with a lodash-style interface.",
  "main": "lib/bundle.js",
  "module": "lib/bundle.es.js",
  "scripts": {
    "compile": "BABEL_ENV=build rollup -c && BABEL_ENV=build rollup -c --output.format=es --output.file=\"lib/bundle.es.js\"",
    "preversion": "npm test",
    "version": "npm run compile",
    "test": "BABEL_ENV=test mocha --require @babel/register",
    "test:types": "dtslint types",
    "inspect": "BABEL_ENV=test mocha --require babel-core/register --inspect",
    "typings": "run-s typings:compile-transform typings:create-header typings:generate-typings typings:prettier",
    "typings:debug": "run-s typings:compile-transform typings:generate-typings:debug",
    "typings:compile-transform": "BABEL_ENV=build babel scripts/typings/index.js > scripts/compiled/typings.js",
    "typings:generate-typings:debug": "node --inspect ./node_modules/jscodeshift/bin/jscodeshift.sh -s --run-in-band -t scripts/compiled/typings.js src/utils/list/index.js",
    "typings:create-header": "cat scripts/typings/index.template.d.ts > types/index.d.ts",
    "typings:generate-typings": "jscodeshift -s -t scripts/compiled/typings.js src/utils/list/index.js >> types/index.d.ts",
    "typings:prettier": "prettier --write types/index.d.ts"
  },
  "bin": {
    "shades": "playground/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jamesmcnamara/shades.git"
  },
  "keywords": [
    "lens",
    "lodash",
    "functional",
    "immutable",
    "cursor",
    "reducer",
    "profunctor",
    "state management"
  ],
  "author": "James McNamara",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jamesmcnamara/shades/issues"
  },
  "homepage": "https://github.com/jamesmcnamara/shades#readme",
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/parser": "^7.1.3",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-syntax-flow": "^7.0.0",
    "@babel/plugin-transform-flow-strip-types": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "chai": "^4.2.0",
    "dtslint": "^0.3.0",
    "immutable": "^3.8.2",
    "jscodeshift": "^0.5.1",
    "lodash": "^4.17.11",
    "mocha": "5.0.4",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.3",
    "rollup": "^0.56.5",
    "rollup-plugin-babel": "^4.0.1",
    "rollup-plugin-cleanup": "^3.0.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "typescript": "^3.1.3",
    "typescript-cli": "^0.1.0"
  }
}
